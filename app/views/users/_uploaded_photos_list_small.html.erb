<ul id="user-photos-uploaded-small" class="contains-floats">
  <% photos.each do |photo| %>
  <li>
    <% if logged_in? %>
    <%= link_to(image_tag(photo.data.url(:small)), photo.data.url(:large), :rel => "user_image_group") %>
    <% else %>
    <%= link_to(image_tag(photo.data.url(:small)), login_path) %>
    <% end %>
  </li>
  <% end %>
</ul>